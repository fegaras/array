#/bin/bash

ARRAY_HOME="$(cd `dirname $0`/../..; pwd -P)"

if [ -z ${SPARK_HOME} ]; then
   SPARK_HOME=~/spark-2.2.0-bin-hadoop2.6
fi

export HADOOP_CONF_DIR=

jar cf test.jar -C classes .

${SPARK_HOME}/bin/spark-submit --jars ${ARRAY_HOME}/lib/array.jar --class $1 --master local[2] test.jar $2 $3 $4 $5 $6
