#/bin/bash

ARRAY_HOME="$(cd `dirname $0`/../..; pwd -P)"

if [ -z ${SPARK_HOME} ]; then
   SPARK_HOME=~/spark-3.0.0-bin-hadoop2.7
fi

export HADOOP_CONF_DIR=

jar cf test.jar -C classes .


${SPARK_HOME}/bin/spark-submit --jars ${ARRAY_HOME}/lib/array.jar --class $1 --driver-memory 8g --executor-memory 1G --master local[*] test.jar $2 $3 $4 $5 $6
